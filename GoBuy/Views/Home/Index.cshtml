@model IEnumerable<GoBuy.Models.Category>
@{
    Layout = "~/Views/_Shared/_LayoutPage1.cshtml";
}
@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>

@if (Model.Count() == 0)
{
    <p>目前無任何商品</p>
}
else
{
    foreach (var category in Model)
    {
        <p>
            <label>@category.CategoryId</label>
            <span>
                <a href="Browse?categoryId=@category.CategoryId">@category.CategoryName</a>
            </span>
        </p>
    }
}



